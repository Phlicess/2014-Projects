@{
    ViewBag.Title = "登录";
}

@section featured {
<section class="featured">
    <form name="LoginForm" method="post" action="">
        <table>
            <tr>
                <td>登录帐号:<input type="text" name="Name" id="Name"></td>
            </tr>
            <tr>
                <td>登录密码:<input type="password" name="Password" id="Password"></td>
            </tr>
            <tr>
                <td>
                    <input type="submit" value="登录" style="background-color: #000000">
                    <input type="reset" value="重置" style="background-color: #000000">
                </td>
            </tr>
        </table>
    </form>
</section>
}
